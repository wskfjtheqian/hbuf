package go = "parser"
package java = "com.parser"

//12
enum Status {
    //启用
    Enable = 0

    //禁用
    Disabled = 1
}

//12
data GetInfoReq {
    int64? user_id = 1
}

//12
data GetInfoResp {

    int8 v1 = 0

    int16 v2 = 1

    int32 v3 = 2

    int64 v4 = 3

    uint8 v5 = 4

    uint16 v6 = 5

    uint32 v7 = 6

    uint64 v8 = 7

    bool v9 = 8

    float v10 = 9

    double v11 = 10

    string v12 = 11

    date v13 = 12

    decimal v14 = 13

    Status v15 = 14

    GetInfoReq v16 = 15

    GetInfoReq[] v17 = 16

    GetInfoReq<string> v18 = 17
}

//12
server UserServer {
    //12
    GetInfoResp GetInfo(GetInfoReq req) = 0
}

