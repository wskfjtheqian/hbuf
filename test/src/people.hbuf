package "parser"

data Base = 1{

  string?[]? name_people = 1  //姓名

}

//人
data People : Base = 1{

  string?[]? name_people = 1  //姓名

  string?<string>? map = 2  //姓名

}

//服务
server PeopleServer = 1 {
  //获得年龄
  People getPeople(People userId) = 1

  //获得姓名
  People getAddress(People userId) = 2


}

//服务
server StudentServer : PeopleServer  = 2 {
  //获得年龄
  People getPeople(People userId) = 1

  //获得姓名
  People getNumber(People userId) = 2


}