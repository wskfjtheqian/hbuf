package go = "parser"
package java = "com.parser"

//12
enum Status {
    //启用
    Enable = 0

    //禁用
    Disabled = 1
}

//12
data GetInfoReq {
    int64? user_id = 1
}

//12
data GetInfoResp {
    //姓名
    string? name = 1

    //年龄
    int32? age = 2

    //注册时间
    date? register_time = 3

    //状态
    Status? status = 4
}

//12
server UserServer {
    //12
    GetInfoResp GetInfo(GetInfoReq req) = 0
}

